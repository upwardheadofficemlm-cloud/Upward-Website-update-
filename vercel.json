{
  "headers": [
    {
      "source": "/payments",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        }
      ]
    },
    {
      "source": "/payments/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        }
      ]
    },
    {
      "source": "/(.*)\\.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)\\.css",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)\\.(png|jpg|jpeg|gif|svg|ico|webp)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/sw.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Service-Worker-Allowed",
          "value": "/"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/payments",
      "destination": "/index.html"
    },
    {
      "source": "/payments/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/admin",
      "destination": "/index.html"
    },
    {
      "source": "/admin/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/proposals/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/payments",
      "destination": "/index.html",
      "permanent": false
    },
    {
      "source": "/payments/(.*)",
      "destination": "/index.html",
      "permanent": false
    },
    {
      "source": "/admin",
      "destination": "/index.html",
      "permanent": false
    },
    {
      "source": "/admin/(.*)",
      "destination": "/index.html",
      "permanent": false
    },
    {
      "source": "/proposals/digital-marketing/:proposalId",
      "destination": "/index.html",
      "permanent": false
    },
    {
      "source": "/proposals/web-development/:proposalId",
      "destination": "/index.html",
      "permanent": false
    },
    {
      "source": "/proposals/brand-identity/:proposalId",
      "destination": "/index.html",
      "permanent": false
    },
    {
      "source": "/proposals/social-media/:proposalId",
      "destination": "/index.html",
      "permanent": false
    },
    {
      "source": "/proposals/content-creation/:proposalId",
      "destination": "/index.html",
      "permanent": false
    },
    {
      "source": "/proposals/seo-services/:proposalId",
      "destination": "/index.html",
      "permanent": false
    },
    {
      "source": "/proposals/:proposalType/:proposalId",
      "destination": "/index.html",
      "permanent": false
    }
  ]
}



